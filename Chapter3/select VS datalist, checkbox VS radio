        <option value = 1>짜장면</option>
        <option value = 짬뽕></option>    <!--선택하는 칸에 짬뽕이 없음-->
        <option>탕수육</option>
      </select>
    </form>

    <input type = text list = country>
    <datalist id = country>
      <option value = 가나></option>
      <option value = 스위스>스위스</option> <!-- 스위스 중복 출력 안됨-->
      <option value = 3>브라질</option> <!--3브라질-->
    </datalist>
    
    <form> <!--중복선택가능-->
      짜장면<input type = "checkbox" value =1>
      짬뽕<input type= "checkbox" value =2>
      탕수육<input type = "checkbox" value = 3>
    </form>
    <form>
      <label><input type = radio name = china>짜장면</label>
      <input type=radio name = china>짬뽕
